<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/viewPanner"
        android:layout_width="match_parent"
        android:layout_height="@dimen/oneSeventy_dp"
        android:layout_marginTop="@dimen/ten_dp"
        android:background="@color/shimmer_background_color"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/include8"
        layout="@layout/layout_home_shimmer_effect_single_circle_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewPanner" />

    <include
        android:id="@+id/include9"
        layout="@layout/layout_home_shimmer_effect_single_circle_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/include8"
        app:layout_constraintTop_toBottomOf="@id/viewPanner" />

    <include
        android:id="@+id/include10"
        layout="@layout/layout_home_shimmer_effect_single_circle_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/include9"
        app:layout_constraintTop_toBottomOf="@id/viewPanner" />

    <include
        android:id="@+id/include11"
        layout="@layout/layout_home_shimmer_effect_single_circle_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/include10"
        app:layout_constraintTop_toBottomOf="@id/viewPanner" />

    <include
        android:id="@+id/include12"
        layout="@layout/layout_home_shimmer_effect_single_circle_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/include11"
        app:layout_constraintTop_toBottomOf="@id/viewPanner" />

    <include
        android:id="@+id/include13"
        layout="@layout/layout_home_shimmer_effect_single_circle_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/include12"
        app:layout_constraintTop_toBottomOf="@id/viewPanner" />

    <include
        android:id="@+id/include15"
        layout="@layout/layout_home_shimmer_effect_single_horizontal_product"
        android:layout_width="@dimen/oneThirtyFive_dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/fifteen_dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/include8" />

    <include
        android:id="@+id/include16"
        layout="@layout/layout_home_shimmer_effect_single_horizontal_product"
        android:layout_width="@dimen/oneThirtyFive_dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/twenty_dp"
        android:layout_marginTop="@dimen/fifteen_dp"
        app:layout_constraintStart_toEndOf="@id/include15"
        app:layout_constraintTop_toBottomOf="@id/include8" />

    <include
        android:id="@+id/include17"
        layout="@layout/layout_home_shimmer_effect_single_horizontal_product"
        android:layout_width="@dimen/oneThirtyFive_dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/twenty_dp"
        android:layout_marginTop="@dimen/fifteen_dp"
        app:layout_constraintStart_toEndOf="@id/include16"
        app:layout_constraintTop_toBottomOf="@id/include8" />








    <include
        android:id="@+id/include18"
        layout="@layout/layout_home_shimmer_effect_single_horizontal_product"
        android:layout_width="@dimen/oneThirtyFive_dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/fifteen_dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/include15" />

    <include
        android:id="@+id/include19"
        layout="@layout/layout_home_shimmer_effect_single_horizontal_product"
        android:layout_width="@dimen/oneThirtyFive_dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/twenty_dp"
        android:layout_marginTop="@dimen/fifteen_dp"
        app:layout_constraintStart_toEndOf="@id/include15"
        app:layout_constraintTop_toBottomOf="@id/include16" />

    <include
        android:id="@+id/include20"
        layout="@layout/layout_home_shimmer_effect_single_horizontal_product"
        android:layout_width="@dimen/oneThirtyFive_dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/twenty_dp"
        android:layout_marginTop="@dimen/fifteen_dp"
        app:layout_constraintStart_toEndOf="@id/include16"
        app:layout_constraintTop_toBottomOf="@id/include17" />

</androidx.constraintlayout.widget.ConstraintLayout>